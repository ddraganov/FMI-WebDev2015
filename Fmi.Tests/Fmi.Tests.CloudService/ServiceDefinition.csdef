<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="Fmi.Tests.CloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-06.2.4">
  <WebRole name="Fmi.Tests.Api.Services" vmsize="ExtraSmall">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" />
    </Endpoints>
    <ConfigurationSettings>
      <Setting name="testsConnection" />
    </ConfigurationSettings>
  </WebRole>
  <WebRole name="RestTestWebApp" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="Endpoint1" endpointName="Endpoint1" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="8080" />
    </Endpoints>
    <ConfigurationSettings>
      <Setting name="ApiBaseUrl" />
    </ConfigurationSettings>
  </WebRole>
</ServiceDefinition>